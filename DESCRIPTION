Package: statGraph
Type: Package
Title: Statistical Methods for Graphs
Version: 1.0.2
Authors@R: c(
    person("Grover E.", "Castro Guzman", email="grover@usp.br", role=c("aut")), 
    person("Diogo R.", "da Costa", email="diogocost2@gmail.com", role=c("aut")),
